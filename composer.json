{
    "name": "urbanraccoon/laratrust-ingest",
    "description": "Utility package to ingest roles and permissions easily using command line.",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [{
        "name": "UrbanRaccoon",
        "email": "pieter.marchand@yeastconsulting.be"
    }],
    "require": {
        "php": ">=7.2.0",
        "santigarcor/laratrust": ">5.0",
        "illuminate/support": "^8.0",
        "urbanraccoon/laraviz": "dev-dev-urban"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Vizrex\\LaratrustIngest\\": "src/Providers"
        }
    },
    "extra": {
